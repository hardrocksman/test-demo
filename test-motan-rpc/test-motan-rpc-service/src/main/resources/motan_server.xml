<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:motan="http://api.weibo.com/schema/motan"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
   http://api.weibo.com/schema/motan http://api.weibo.com/schema/motan.xsd">

    <motan:registry regProtocol="consul" name="my_consul" address="10.0.4.110:8500"/>

    <!-- service implemention bean -->
    <bean id="serviceImpl" class="com.zhl.test.motan.rpc.service.FooService" />
    <!-- exporting service by motan -->
    <!--<motan:service interface="com.zhl.test.motan.rpc.IFooService" ref="serviceImpl" export="8002" />-->

    <motan:service interface="com.zhl.test.motan.rpc.IFooService" ref="serviceImpl" registry="my_consul" export="8002" />
</beans>